{
    "name": "artisanry/pages",
    "description": "Pages for Laravel 5",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Pages",
        "Laravel Pages"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "cviebrock/eloquent-sluggable": "^4.4",
        "artisanry/taggable": "^1.0",
        "artisanry/parsedown": "^1.0",
        "illuminate/support": "^5.8"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Pages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Pages\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Pages\\PagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
